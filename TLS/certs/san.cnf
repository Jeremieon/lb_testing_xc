[req]
default_bits = 4096
prompt = no
default_md = sha256
distinguished_name = dn
req_extensions = req_ext

[dn]
C=US
ST=Washington
L=Seattle
O=F5 Lab Testing
OU=Solution Engineering
CN=labtestdemo.com

[req_ext]
subjectAltName = @alt_names

[alt_names]
DNS.1 = labtestdemo.com
DNS.2 = *.labtestdemo.com
DNS.3 = *.*.labtestdemo.com
DNS.4 = localhost
IP.1 = 127.0.0.1

